<% set_meta_tags title: '組織登録申請一覧' %>
<div id="titlebar">
  <div class="row">
    <div class="col-md-12">
      <h2><i class="sl sl-icon-list"></i> 組織登録申請一覧</h2>
    </div>
  </div>
</div>

<div class="row">

  <!-- Listings -->
  <div class="col-lg-12 col-md-12">
    <div class="messages-container margin-top-0">
      <% if @organization_registrations.empty? %>
        <div class="messages-headline">
          <h4>申請された組織登録はありません</h4>
        </div>
      <% elsif %>
        <div class="messages-inbox">
          <ul>
            <%= render @organization_registrations %>
          </ul>
        </div>
      <% end %>
    </div>

    <div class="clearfix"></div>
    <div class="pagination-container margin-top-30 margin-bottom-0">
      <%= paginate @organization_registrations %>
    </div>
  </div>
