<!DOCTYPE html>
<html>
  <head>
    <%= render 'shared/head_tag' %>
  </head>

  <body>

    <!-- Wrapper -->
    <div id="wrapper">

      <!-- Header Container -->
      <header id="header-container" class="fixed fullwidth">
        <!-- Header -->
        <div id="header" class="not-sticky">
          <div class="container">
            <%= render 'shared/header' %>
          </div>
        </div>
        <!-- Header / End -->
      </header>

      <div class="clearfix"></div>
      <!-- Header Container / End -->

      <%= yield %>

    </div>
    <!-- Wrapper / End -->

    <%= javascript_include_tag 'application' %>

    <!-- Maps -->
    <script src="https://maps.googleapis.com/maps/api/js?key=<%= Rails.application.credentials[:google_maps][:api_key] %>&callback=initMap&v=weekly"></script>
    <%= javascript_include_tag 'infobox.min' %>
    <%= javascript_include_tag 'markerclusterer' %>
    <%= javascript_include_tag 'maps' %>
  </body>
</html>